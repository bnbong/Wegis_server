# Application Settings
ENVIRONMENT=development
PROJECT_NAME="Wegis Server"
SECRET_KEY=your-secret-key-here

# CORS Settings
CLIENT_ORIGIN=http://localhost:3000
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:9000

# PostgreSQL Database
POSTGRES_USER=admin
POSTGRES_PASSWORD=password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=phishing_data
POSTGRES_POOL_SIZE=5
POSTGRES_MAX_OVERFLOW=10

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_TTL=43200
REDIS_NAMESPACE=wegis
REDIS_MAX_CONNECTIONS=10
REDIS_RETRY_ON_TIMEOUT=true
REDIS_SOCKET_TIMEOUT=5

# MongoDB Database
MONGODB_USER=admin
MONGODB_PASSWORD=password
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_NAME=phishing_feedback
MONGODB_MAX_POOL_SIZE=10
MONGODB_MIN_POOL_SIZE=1
MONGODB_SERVER_SELECTION_TIMEOUT=5000

# AI Model Settings
MODEL_NAME=best_acc_model.pt
HTML_LOAD_TIMEOUT=20
HTML_LOAD_RETRIES=2
MAX_CONCURRENT_REQUESTS=5
MODEL_CACHE_SIZE=1

# Chrome/Selenium Settings
CHROME_BIN=/usr/bin/chromium
CHROMEDRIVER_PATH=/usr/bin/chromedriver
CHROME_PROCESS_TIMEOUT=30
CHROME_MAX_INSTANCES=2

# Domain Management
DOMAIN_ENABLE_PATTERNS=true
