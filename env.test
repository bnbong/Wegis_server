# Application Settings
ENVIRONMENT=test
PROJECT_NAME="Wegis Server Test"
SECRET_KEY=test-secret-key

# CORS Settings (permissive for testing)
CLIENT_ORIGIN=http://localhost:3000
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:9000

# PostgreSQL Database (Test)
POSTGRES_USER=test_admin
POSTGRES_PASSWORD=test_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
POSTGRES_DB=test_phishing_data
POSTGRES_POOL_SIZE=2
POSTGRES_MAX_OVERFLOW=5

# Redis Cache (Test)
REDIS_HOST=localhost
REDIS_PORT=6380
REDIS_DB=0
REDIS_CACHE_TTL=300
REDIS_NAMESPACE=wegis_test
REDIS_MAX_CONNECTIONS=5
REDIS_RETRY_ON_TIMEOUT=true
REDIS_SOCKET_TIMEOUT=3

# MongoDB Database (Test)
MONGODB_USER=test_admin
MONGODB_PASSWORD=test_password
MONGODB_HOST=localhost
MONGODB_PORT=27018
MONGODB_NAME=test_phishing_feedback
MONGODB_MAX_POOL_SIZE=5
MONGODB_MIN_POOL_SIZE=1
MONGODB_SERVER_SELECTION_TIMEOUT=3000

# AI Model Settings (Test)
MODEL_NAME=best_acc_model.pt
HTML_LOAD_TIMEOUT=10
HTML_LOAD_RETRIES=1
MAX_CONCURRENT_REQUESTS=2
MODEL_CACHE_SIZE=1

# Chrome/Selenium Settings (Test)
CHROME_BIN=/usr/bin/chromium
CHROMEDRIVER_PATH=/usr/bin/chromedriver
CHROME_PROCESS_TIMEOUT=15
CHROME_MAX_INSTANCES=1

# Domain Management
DOMAIN_ENABLE_PATTERNS=true
